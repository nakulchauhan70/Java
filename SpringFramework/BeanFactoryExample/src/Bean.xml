<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean id="message" class="com.demo.model.Message" scope="prototype">
		<property name="messageId" value="100" />
		<property name="msg" value="This is msg"></property>
	</bean>

	<alias name="message" alias="msgalias1" />
	<alias name="message" alias="msgalias2" />

</beans>